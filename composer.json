{
  "name": "kkgerry/tiktokshop-od-larevel",
  "description": "Unofficial Tiktok Shop API Client in Laravel",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Gerry",
      "email": "26186440@qq.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2|^8.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.3|^6.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^8|^9"
  },
  "autoload": {
    "psr-4": {
      "Kkgerry\\TiktokShop\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kkgerry\\TiktokShop\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
	"branch-alias": {
	  "dev-master": "1.0.x-dev"
	}
  }
}
